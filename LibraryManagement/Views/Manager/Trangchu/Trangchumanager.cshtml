
@{
    ViewBag.Title = "Trangchumanager";
    Layout = "~/Views/Shared/ManagerMasterPage.cshtml";
}

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="content all-icons">
                        <div class="row">
                            <div class="font-icon-list col-lg-2 col-md-3 col-sm-4 col-xs-6 col-xs-6">
                                <div class="font-icon-detail">
                                    <i class="pe-7s-map"></i>
                                    <input type="text" value="Quản lý sách" onclick="QuanLySach/Xem">
                                </div>
                            </div>
                            <div class="font-icon-list col-lg-2 col-md-3 col-sm-4 col-xs-6 col-xs-6">
                                <div class="font-icon-detail">
                                    <i class="pe-7s-users"></i>
                                    <input type="text" value="Quản lý người dùng" onclick="QuanLyUser/Xem">
                                </div>
                            </div>
                            <div class="font-icon-list col-lg-2 col-md-3 col-sm-4 col-xs-6 col-xs-6">
                                <div class="font-icon-detail">
                                    <i class="pe-7s-id"></i>
                                    <input type="text" value="Quản lý manager" onclick="QuanLyManager/Xem">
                                </div>

                            </div>
                            <div class="font-icon-list col-lg-2 col-md-3 col-sm-4 col-xs-6 col-xs-6">
